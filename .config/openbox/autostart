xrandr | grep DVI1 | grep " connected "
if [ $? -eq 0 ]; then
  xrandr --output LVDS1 --off
  xrandr --output DVI1 --mode 1680x1050
  xrandr --output DVI1 --left-of LVDS1
fi
nitrogen --restore
compton -bCGfF -i 0.95 --sw-opti -I 0.09 -O 0.09
sleep 1 && guake &
#tint2 &
#mopidy &
volti &
rofi -key-run F12
