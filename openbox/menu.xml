					<execute>arandr</execute>
				</action>
			</item>
			<item label="Bluetooth Manager">
				<action name="Execute">
					<execute>python /usr/bin/blueman-manager</execute>
				</action>
			</item>
			<item label="Look &amp; Feel">
				<action name="Execute">
